{
    "name": "rinvex/authy",
    "description": "Rinvex Authy is a simple wrapper for Authy TOTP API, the best rated Two-Factor Authentication service for consumers, simplest 2fa Rest API for developers and a strong authentication platform for the enterprise.",
    "keywords": [
        "sms",
        "call",
        "authy",
        "message",
        "security",
        "twofactor",
        "verification",
        "authentication",
        "automated",
        "register",
        "rinvex",
        "phone",
        "token"
    ],
    "license": "MIT",
    "homepage": "https://rinvex.com",
    "support": {
        "email": "help@rinvex.com",
        "issues": "https://github.com/rinvex/authy/issues",
        "source": "https://github.com/rinvex/authy",
        "docs": "https://github.com/rinvex/authy/README.md"
    },
    "authors": [
        {
            "name": "Rinvex LLC",
            "homepage": "https://rinvex.com",
            "email": "help@rinvex.com"
        },
        {
            "name": "Abdelrahman Omran",
            "homepage": "https://omranic.com",
            "email": "me@omranic.com",
            "role": "Project Lead"
        },
        {
            "name": "The Generous Laravel Community",
            "homepage": "https://github.com/rinvex/authy/contributors"
        }
    ],
    "require": {
        "php": "^7.0.0",
        "psr/http-message": "^1.0.0",
        "guzzlehttp/guzzle": "^6.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.4.0",
        "mockery/mockery": "^0.9.5"
    },
    "autoload": {
        "psr-4": {
            "Rinvex\\Authy\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rinvex\\Authy\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
